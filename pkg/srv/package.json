{
  "name": "@kmcssz-org/scoutdb-srv",
  "version": "1.0.0",
  "description": "The back-end component of the KMCSSZ ScoutDB.",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "type": "commonjs",
  "repository": "https://github.com/scoutdb/scoutdb.git",
  "keywords": [],
  "scripts": {
    "start": "node build/scoutdb-cli.js --log-level=debug --tracing-diag-log-level=NONE --www-dir=$PWD/../gui/www/",
    "build": "run-p 'build:*'",
    "build:main": "tsc -p tsconfig.json",
    "codegen": "openapi-generator-cli generate -i ../common/src/openapi.json -g typescript-angular -o ./generated/openapi/typescript-angular/ --additional-properties=ngVersion=18.0.0,npmName=restClient,supportsES6=true,npmVersion=10.2.4,withInterfaces=true",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "run-s build test:*",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:unit": "nyc --silent ava",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.json -w",
    "watch:test": "nyc --silent ava --watch",
    "cov": "run-s build test:unit cov:html cov:lcov && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:lcov": "nyc report --reporter=lcov",
    "cov:send": "run-s cov:lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@ava/typescript": "4.1.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@openapitools/openapi-generator-cli": "2.13.4",
    "@types/express": "4.17.18",
    "@types/nodemailer": "^6.4.11",
    "@types/sendmail": "^1.4.5",
    "@types/yargs": "17.0.28",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "ava": "5.3.1",
    "codecov": "3.8.3",
    "cspell": "7.3.7",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-functional": "6.0.0",
    "eslint-plugin-import": "2.28.1",
    "gh-pages": "6.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "open-cli": "7.2.0",
    "prettier": "3.0.3",
    "standard-version": "9.5.0",
    "ts-node": "10.9.1",
    "typedoc": "0.25.1",
    "typescript": "5.4.5"
  },
  "files": [
    "build",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "src/": "build/"
      }
    },
    "files": [
      "!build/module/**"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": false
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "dependencies": {
    "@kmcssz-org/scoutdb-common": "0.0.1",
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/auto-instrumentations-node": "0.39.2",
    "@opentelemetry/core": "1.17.0",
    "@opentelemetry/instrumentation-pino": "0.34.1",
    "@opentelemetry/sdk-metrics": "1.17.0",
    "@opentelemetry/sdk-node": "0.43.0",
    "@opentelemetry/semantic-conventions": "1.17.0",
    "express": "4.18.2",
    "express-openapi-validator": "5.1.6",
    "http-errors-enhanced": "2.0.7",
    "knex": "3.1.0",
    "pino": "8.15.6",
    "pino-opentelemetry-transport": "0.4.0",
    "pino-pretty": "10.2.3",
    "sqlite3": "5.1.7",
    "ts-results": "3.3.0",
    "tslib": "2.6.2",
    "yargs": "17.7.2"
  }
}
